{
  "name": "exchange-shielded-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for cryptocurrency exchanges to process Zcash shielded withdrawals securely and compliantly",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./address-validator": {
      "import": "./dist/address-validator.js",
      "types": "./dist/address-validator.d.ts"
    },
    "./transaction-builder": {
      "import": "./dist/transaction-builder.js",
      "types": "./dist/transaction-builder.d.ts"
    },
    "./rpc-client": {
      "import": "./dist/rpc-client.js",
      "types": "./dist/rpc-client.d.ts"
    },
    "./security": {
      "import": "./dist/security/index.js",
      "types": "./dist/security/index.d.ts"
    },
    "./compliance": {
      "import": "./dist/compliance/index.js",
      "types": "./dist/compliance/index.d.ts"
    },
    "./sdk": {
      "import": "./dist/sdk/index.js",
      "types": "./dist/sdk/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src tests",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "prepare": "npm run build"
  },
  "keywords": [
    "zcash",
    "cryptocurrency",
    "address-validation",
    "shielded",
    "exchange",
    "sapling",
    "orchard",
    "unified-address",
    "z-address",
    "privacy",
    "blockchain",
    "crypto",
    "withdrawal",
    "transaction",
    "rpc",
    "compliance",
    "audit",
    "security",
    "rate-limiting"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/example/exchange-shielded-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/example/exchange-shielded-sdk/issues"
  },
  "homepage": "https://github.com/example/exchange-shielded-sdk#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.51.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "bech32": "^2.0.0",
    "bs58check": "^4.0.0"
  }
}
